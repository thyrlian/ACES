{
  "manifest_version": 2,

  "name": "ACES",
  "description": "A Chrome Extension Scaffold",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }]
}